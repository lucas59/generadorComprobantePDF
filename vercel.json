{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node",
        "config": { "maxLambdaSize": "10mb" },
        "env": {
          "NODE_ENV": "production"
        },
        "target": "es2017"

    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}